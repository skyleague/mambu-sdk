{
  "name": "@skyleague/mambu-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK generated from the Mambu API v2 spec.",
  "repository": {
    "type": "git",
    "url": "https://github.com/skyleague/mambu-sdk.git"
  },
  "license": "MIT",
  "author": "SkyLeague Technologies B.V. <technologies@skyleague.io> (https://skyleague.io)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./.dist/index.js",
    "./package.json": "./package.json",
    "./*.js": "./.dist/*.js"
  },
  "types": "./.dist/index.d.ts",
  "files": [".dist", "package.json"],
  "scripts": {
    "build": "tsc -p tsconfig.dist.json",
    "build:index": "node --import tsx/esm scripts/build-index.ts",
    "check:coverage": "vitest run --coverage=true",
    "check:project": "node-standards lint",
    "check:types": "tsc -p tsconfig.json",
    "lint": "biome check",
    "lint:fix": "node --run lint -- --write --unsafe",
    "package": "rm -rf .dist && node --run build",
    "prepare": "husky || true",
    "test": "vitest run",
    "prebuild:docs": "npm ci"
  },
  "dependencies": {
    "split2": "^4.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@skyleague/axioms": "^6.5.1",
    "@skyleague/node-standards": "^9.6.0",
    "@skyleague/therefore": "^7.12.0",
    "@types/split2": "^4.2.3",
    "camelcase": "^8.0.0",
    "ky": "^1.7.5",
    "nock": "14.0.0-beta.19",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "ky": "^1.7.4"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  },
  "node-standards": {
    "extends": "library"
  }
}
